   "source": [
    "# Filter\n",
    "\n",
    "## difference between filter() and map() \n",
    "\n",
    "### filter(func,interables) will only return TRUE values whereas map(func,iterables) will return both TRUE and FALSE "
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 2,
   "id": "31bc3215",
   "metadata": {},
   "outputs": [
    {
     "data": {
